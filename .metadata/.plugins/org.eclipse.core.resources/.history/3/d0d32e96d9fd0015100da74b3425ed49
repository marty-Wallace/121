import web.Crawler;

public class Driver {
	
	
	public static void main(String[]args) {
		
		String urlDogs = "https://www.google.ca/search?q=640+x+480+image+size&source=lnms&tbm=isch&sa=X&ved=0ahUKEwi8y8TSioDMAhWKnIMKHXenDi8Q_AUIBygB&biw=1184&bih=542#tbm=isch&q=640+x+480+wallpaper";
		//new MosaicGUI();
		new Crawler().scrapeImages(urlDogs, "640x480/");
	}

}
